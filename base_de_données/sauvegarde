bash
sauvegarde
pg_dump -U ton_utilisateur -h ton_hôte -F c -b -v -f "sicroom_backup.dump" nom_de_la_base
ex :
pg_dump -h localhost -U postgres -F p -v -f backup_lasicroom2.sql lasicroom2 // sauvegarder dans le dossier courant.

restauration
pg_restore -U ton_utilisateur -h ton_hôte -d nom_de_la_base -v "sicroom_backup.dump"
